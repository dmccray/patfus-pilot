<p id="notice"><%= notice %></p>

<h1>Listing Questionaire Sections</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @questionaire_sections.each do |questionaire_section| %>
      <tr>
        <td><%= link_to 'Show', questionaire_template_questionaire_section(@qt, questionaire_section) %></td>
        <td><%= link_to 'Edit', edit_questionaire_template_questionaire_section_path(@qt) %></td>
        <td><%= link_to 'Destroy', questionaire_template_questionaire_section, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Questionaire section', new_questionaire_template_questionaire_section_path %>
